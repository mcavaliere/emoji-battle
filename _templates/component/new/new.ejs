---
to: components/<%= [h.camelizedBaseName(name), h.camelizedBaseName(name)].join('/') %>.tsx
---
<% formattedPath = h.camelizedPathName(name) -%>
<% component = h.camelizedBaseName(name) -%>
import React from 'react';
import { Heading, Center } from '@chakra-ui/react';

export type <%= component %>Props = {
  // Add props here.
  myProp: string;
}

/** Description of component */
export function <%= component %>({ myProp }: <%= component %>Props): JSX.Element {
  const message = '<%= component %> Created.';

  return (
    <Center bg="red.300" color="white" padding={{ base: 16, lg: 24 }}>
      <Heading>{message}</Heading>
    </Center>
  );
};